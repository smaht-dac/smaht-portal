    "protocols": {
        "protocols": {
            "title": "Protocols",
            "description": "Protocols providing experimental details",
            "type": "array",
            "minItems": 1,
            "uniqueItems": true,
            "permission": "restricted_fields",
            "items": {
                "type": "string",
                "linkTo": "Protocol"
            }
        }
    },
    "reads_details": {
        "read_count": {
            "title": "Submitted Read Count",
