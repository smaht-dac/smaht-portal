{
    "title": "Output File",
    "description": "File produced by bioinformatics pipeline",
    "$id": "/profiles/output_file.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "data_category",
        "data_type",
        "file_format"
    ],
    "anyOf": [
        {
            "required": [
                "submission_centers"
            ]
        },
        {
            "required": [
                "consortia"
            ]
        }
    ],
    "identifyingProperties": [
        "accession",
        "aliases",
        "uuid"
    ],
    "additionalProperties": false,
    "mixinFacets": [
        {
            "$ref": "file.json#/facets"
        }
    ],
    "mixinColumns": [
        {
            "$ref": "file.json#/columns"
        }
    ],
    "properties": {
        "$merge": "encoded:schemas/file.json#/properties",
        "accession": {
            "$merge": "encoded:schemas/mixins.json#/accession/accession",
	    "accessionType": "FI"
        },
        "aliases": {
            "$merge": "encoded:schemas/mixins.json#/aliases/aliases"
        },
        "alternate_accessions": {
            "$merge": "encoded:schemas/mixins.json#/accession/alternate_accessions"
        },
        "consortia": {
            "$merge": "encoded:schemas/mixins.json#/attribution/consortia"
        },
        "date_created": {
            "$merge": "encoded:schemas/mixins.json#/submitted/date_created"
        },
        "description": {
            "$merge": "encoded:schemas/mixins.json#/description/description"
        },
        "extra_files": {
            "$merge": "encoded_core:schemas/file.json#/properties/extra_files"
        },
        "last_modified": {
            "$merge": "encoded:schemas/mixins.json#/modified/last_modified"
        },
        "schema_version": {
            "$merge": "encoded:schemas/mixins.json#/schema_version/schema_version",
            "default": "1"
        },
        "submission_centers": {
            "$merge": "encoded:schemas/mixins.json#/attribution/submission_centers"
        },
        "submitted_by": {
            "$merge": "encoded:schemas/mixins.json#/submitted/submitted_by"
        },
        "tags": {
            "$merge": "encoded:schemas/mixins.json#/tags/tags"
        },
        "uuid": {
            "$merge": "encoded:schemas/mixins.json#/uuid/uuid"
        },
        "variant_type": {
            "$merge": "encoded:schemas/mixins.json#/variant_type/variant_type"
        }
    }
}
