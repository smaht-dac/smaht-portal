{
    "title": "New Type",
    "$id": "/profiles/new_type.json",
        "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "submission_centers",
        "consortia",
        "last_modified"
    ],
    "identifyingProperties": [
        "uuid"
    ],
    "additionalProperties": false,
    "mixinProperties": [
        {
            "$ref": "mixins.json#/attribution"
        },
        {
            "$ref": "mixins.json#/modified"
        },
        {
            "$ref": "mixins.json#/schema_version"
        }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "date": {
            "title": "Date",
            "description": "string property that must be formatted as a date or datetime",
            "type": "string",
            "anyOf": [
                {
                    "format": "date-time"
                },
                {
                    "format": "date"
                }
            ]
        },
        "foo_or_bar": {
            "title": "Foo or Bar",
            "description": "string property that only accepts Foo or Bar",
            "type": "string",
            "enum": [
                "Foo",
                "Bar"
            ]
        },
        "integer_4_to_50": {
            "title": "Integer -4 to 50",
            "description": "integer property limited to greater than -4 and le 50",
            "type": "integer",
            "maximum": 50,
            "exclusiveMinimum": -4
        },
        "object_with_add_properties": {
            "title": "Object With Additional Properties",
            "description": "An objects that allows additional properties",
            "type": "object",
            "properties": {
                "key1": {
                    "type": "number"
                },
                "key2": {
                    "type": "string"
                }
            },
            "additionalProperties": true
        },
        "object_without_add_properties": {
            "title": "Object Without Additional Properties",
            "description": "An objects that does not allow additional properties",
            "type": "object",
            "properties": {
                "key1": {
                    "type": "number"
                },
                "key2": {
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "number_string": {
            "title": "Number String",
            "description": "regex pattern that only accepts number strings",
            "type": "string",
            "pattern": "^[0-9]+$"
        },
        "submission_centers": {
            "description" : "Foo bar"
        },
        "unique_array": {
            "title": "Unique Array",
            "description": "array property of no more than 3 unique entries",
            "type": "array",
            "maxItems": 3,
            "uniqueItems": true,
            "items": {
                "type": "string"
            }
        },
        "urls": {
            "$merge": "encoded_core:schemas/document.json#/properties/urls"
        }
    }
}